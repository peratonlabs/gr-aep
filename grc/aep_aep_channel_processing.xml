<block>
  <name>AEP Channel Processing</name>
  <key>aep_aep_channel_processing</key>
  <category>[AEP]</category>
  <import>import aep</import>
  <make>aep.aep_channel_processing($M, $num_sc, $thresh)</make>
  <param>
    <name>Num Apertures</name>
    <key>M</key>
    <type>int</type>
  </param>
  <param>
    <name>Num Subcarriers</name>
    <key>num_sc</key>
    <type>int</type>
  </param>
  <param>
    <name>Threshold</name>
    <key>thresh</key>
    <type>float</type>
  </param>
  <sink>
    <name>aep_metric</name>
    <type>float</type>
    <vlen>$num_sc</vlen>
  </sink>
  <sink>
    <name>aep_weights</name>
    <type>complex</type>
    <vlen>$M*$num_sc</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$M*$num_sc</vlen>
  </source>
</block>
